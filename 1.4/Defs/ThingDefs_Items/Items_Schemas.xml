		<comps>
			<li Class="ItsSorceryFramework.CompProperties_SchemaItem">
				<compClass>ItsSorceryFramework.CompSchemaItem</compClass>
				<useJob>UseItem</useJob>
				<useLabel>"Coomsume" the foundations of {0}</useLabel>
				<schemaDef>ISFE_Coomsumerism</schemaDef>
			</li>
			<li Class="ItsSorceryFramework.CompProperties_UseEffectSchema">
				<compClass>ItsSorceryFramework.CompUseEffect_Schema</compClass>
				<schemaDef>ISFE_Coomsumerism</schemaDef>
			</li>
			<li Class="CompProperties_UseEffectArtifact">
				<sound>PsychicAnimalPulserCast</sound>
			</li>
		</comps>
	</ThingDef>
	
	<ThingDef ParentName="SchemaItemBase">
